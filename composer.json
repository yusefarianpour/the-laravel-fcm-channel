{
    "name": "yusefarianpour/the-laravel-fcm-channel",
    "description": "The Laravel FCM Channel",
    "keywords": ["laravel", "firebase", "fcm"],
    "license": "MIT",
    "type": "project",
    "authors": [
      {
        "name": "Yusef Arianpour",
        "email": "info@asign.ir"
      }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Yusef\\Channels\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yusef\\Channels\\FirebaseServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
