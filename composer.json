{
    "name": "journalctl/laravel-to-fcm",
    "description": "Send notification from laravel to firebase cloud messaging.",
    "keywords": ["laravel", "firebase", "fcm", "channel"],
    "license": "MIT",
    "type": "project",
    "authors": [
      {
        "name": "Yusef Arianpour",
        "email": "info@asign.ir"
      }
    ],
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "autoload": {
        "psr-4": {
            "Journalctl\\Channels\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Journalctl\\Channels\\FirebaseServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
